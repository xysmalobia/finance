CREATE TABLE users (id INTEGER, username TEXT NOT NULL, hash TEXT NOT NULL, cash NUMERIC NOT NULL DEFAULT 10000.00, PRIMARY KEY(id));
CREATE TABLE 'shares' ('symbol' TEXT, 'name' TEXT, 'user_id' integer, 'total_share_count' integer, 'current_price' number);
CREATE TABLE 'history' ('symbol' TEXT, 'name' TEXT, 'user_id' integer, 'transaction_share_count' integer, 'original_price' number, 'date' datetime, 'purchase' text);
.schema
quit()
